<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.3.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Sphinx4SOC" xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd">

  <bannerLeft>
    <!--         Reflow Maven Skin, but with "Reflow" highlighted -->
    <name><![CDATA[
    <span class="color-highlight">Modern Java Project Examples</span>
   ]]>
    </name>
    <href>file:///media/Work/online/deploy/index.html</href>
  </bannerLeft>

  <publishDate position="bottom" format="yyyy-MM-dd"/>
  <version position="bottom"/>

  <skin>
    <groupId>lt.velykis.maven.skins</groupId>
    <artifactId>reflow-maven-skin</artifactId>
    <version>1.1.1</version>
  </skin>

  <googleAnalyticsAccountId>UA-54501187-1</googleAnalyticsAccountId>

  <custom>
    <reflowSkin>
      <!-- Make this to 'false' for local development, i.e. file:// URLs -->
      <protocolRelativeURLs>false</protocolRelativeURLs>
      <smoothScroll>true</smoothScroll>
      <theme>bootswatch-cosmo</theme>
      <highlightJs>true</highlightJs>
      <highlightJsTheme>github</highlightJsTheme>
      <!-- <absoluteResourceURL>http://gtsntzs.github.io/reflow-maven-skin/</absoluteResourceURL> -->
      <brand>
        <!-- Brand text in top-left part of the site -->
        <name><![CDATA[<span class="color-highlight">Java Project</span> Examples]]></name>
        <href>file:///media/Work/online/deploy/index.html</href>
      </brand>
      <slogan>Modern Java</slogan>
      <titleTemplate>%2$s | %1$s</titleTemplate>
      <!-- Use Table of Contents at the top of the page (max 6 elements) -->
      <toc>top</toc>
      <tocTopMax>7</tocTopMax>
      <!-- Include the documentation and tools in the top navigation (in addition to links) -->
      <topNav>Projects|Contribute</topNav>
      <!-- Split menus in the bottom navigation -->
      <bottomNav maxSpan="9">
        <column>Download|Contribute</column>
        <column>Parents</column>
        <column>reports</column>
      </bottomNav>
      <bottomDescription>
        This is a very good example collection doing interesting and valuable things.
      </bottomDescription>

      <pages>

        <index project="examples">
          <!-- Override the title -->
          <titleTemplate>Java Examples</titleTemplate>
          <!-- no breadcrumbs on the main index page -->
          <breadcrumbs>false</breadcrumbs>
          <!-- no ToC on the main index page -->
          <toc>false</toc>
          <markPageHeader>true</markPageHeader>
          <sections>
            <columns>2</columns>
            <body/>
            <sidebar />
          </sections>
        </index>

        <info>
          <toc>false</toc>
          <breadcrumbs>false</breadcrumbs>
          <sections>
            <columns>2</columns>
            <body/>
          </sections>
        </info>

        <contribute>
          <breadcrumbs>false</breadcrumbs>
        </contribute>

        <!-- Disable source highlighting for Maven reports -->
        <source-repository>
          <highlightJs>false</highlightJs>
        </source-repository>

        <issue-tracking>
          <highlightJs>false</highlightJs>
          <toc>false</toc>
        </issue-tracking>

        <license>
          <highlightJs>false</highlightJs>
          <toc>false</toc>
        </license>

        <!-- Disable ToC for some Maven reports -->
        <project-info>
          <toc>false</toc>
        </project-info>

        <github-report>
          <toc>false</toc>
        </github-report>

        <advanced>
          <sections>
            <columns>2</columns>
            <body />
            <sidebar />
          </sections>
        </advanced>

        <dependencies>
          <tocTopMax>4</tocTopMax>
        </dependencies>

      </pages>
    </reflowSkin>
  </custom>

  <body>

    <breadcrumbs>
      <item name="Java Examples" href="/"/>
    </breadcrumbs>

    <menu name="Projects" inherit="bottom">
      <item name="Archetypes::Parent" href="archetypes/index.html"/>
    </menu>

    <links>
      <item name="Download" href="https://github.com/gtsntzs/blog/master.zip"/>
      <item name="GitHub project" href="https://github.com/gtsntzs/blog"/>
    </links>

    <menu name="Contribute" inherit="bottom">
      <item name="Issues" href="https://github.com/gtsntzs/blog/issues/"/>
      <item name="Fork on GitHub" href="https://github.com/gtsntzs/blog"/>
    </menu>

    <menu name="Maven documentation" ref="reports" inherit="bottom"/>

  </body>
</project>
